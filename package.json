{
  "name": "fullstack-starter",
  "version": "1.0.0",
  "description": "Production-ready full-stack starter template with NestJS and Angular",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "install:all": "pnpm install -r",
    "dev:api": "pnpm --filter api start:dev",
    "dev:web": "pnpm --filter web start",
    "build:all": "pnpm -r run build",
    "build:api": "pnpm --filter api build",
    "build:web": "pnpm --filter web build:prod",
    "clean": "pnpm -r exec rm -rf node_modules dist .angular && rm -rf node_modules",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose up --build -d",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "nestjs",
    "angular",
    "mongodb",
    "jwt",
    "oauth",
    "fullstack"
  ],
  "author": "",
  "license": "MIT"
}
